{"_id":"ETOuu01QUVQGSyii","name":"Fusil Laser, azimuth","permission":{"default":1,"yWe8HobMv9Lno7mU":3},"type":"weapon","data":{"description":{"value":"<p>Avec son canon court fendu et ses renforts internes, un fusil laser est une arme robuste et fiable.</p>","chat":"","unidentified":""},"source":"Core Rulebook, P. 176","type":"","quantity":1,"bulk":"1","price":425,"level":1,"attuned":false,"equipped":true,"identified":true,"attributes":{"sturdy":true,"customBuilt":false,"size":"medium","dex":{"mod":""},"hp":{"value":6,"max":""},"hardness":{"value":""},"ac":{"value":""}},"activation":{"type":"none","cost":null,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":"","units":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":36,"units":"meter","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d8","fire"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":20,"max":20},"usage":{"value":1,"per":"shot"},"modifiers":[],"container":{"contents":[],"storage":[{"type":"slot","subtype":"fusion","amount":1,"acceptsType":["fusion"],"affectsEncumbrance":true,"weightProperty":"level"}]},"weaponType":"longA","weaponCategory":"laser","special":"","properties":{"two":true,"amm":false,"one":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"reach":false,"sniper":false,"stun":false,"thrown":false,"trip":false,"unwieldy":false,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"hybrid":false,"ignite":false,"indirect":false,"integrated":false,"living":false,"lockdown":false,"mind-affecting":false,"mine":false,"mire":false,"modal":false,"necrotic":false,"polarize":false,"polymorphic":false,"professional":false,"punchGun":false,"radioactive":false,"recall":false,"regrowth":false,"relic":false,"reposition":false,"shape":false,"shells":false,"shield":false,"subtle":false,"sunder":false,"swarm":false,"tail":false,"teleportive":false,"thought":false,"throttle":false,"unbalancing":false,"underwater":false,"variantBoost":false,"wideLine":false},"proficient":false,"abilityMods":{"parts":[]}},"folder":"Ky1ybpFCqVHwBsqy","sort":3700000,"flags":{"itemacro":{"macro":{"data":{"name":"Fusil Laser, azimuth","type":"script","scope":"global","command":"AudioHelper.play({src: \"Sounds/SF/laser long.mp3\", volume: 0.8, autoplay: true, loop: false}, true);\r\nconst kac_damage_types = [`bludgeoning`, `piercing`, `slashing`];\r\nconst cover_types = [[0, `No Cover`],[2, `Partial Cover`], [4, `Normal Cover`], [4, `Soft Cover`], [8, `Improved Cover`], [200, `Total Cover`]];\r\nconst conceal_types = [[0, `No Concealment`], [20, `Normal Concealment`], [50, `Total Concealment`]];\r\n\r\n(async ()=>{\r\n  if(game.user.targets.size !== 1) return ui.notifications.error(`Target 1 Enemy.`);\r\n\r\n  let { eac, kac, fort, reflex, will } = Array.from(game.user.targets)[0]?.actor.data.data.attributes;\r\n  let { damage, save, equipped, capacity} = item.data.data;\r\n\r\n  if(!equipped) return ui.notifications.error(`Cannot attack with an unequipped weapon.`);\r\n  if(capacity.max > 0 && capacity.value === 0) return ui.notifications.error(`Weapon has no ammunition, reload!`);\r\n\r\n  item.rollAttack({event});\r\n  Hooks.once(`preCreateChatMessage`, async (data, options, userID) => {\r\n    if(userID !== game.userID && !data.flavor.includes(item.name)) return;\r\n\r\n    let rollData = JSON.parse(data.roll);\r\n    if(rollData.total < kac.value && rollData.total < eac.value) return; \r\n    let cover = await choose(cover_types, `Does the enemy have cover?`);\r\n    let concealment = await choose (conceal_types, `Is the enemy under any concealment?`);\r\n    let concealment_roll = new Roll(`1d100`).roll().total;\r\n\r\n    let kac_type = false;  \r\n    damage.parts.forEach(part => { if(kac_damage_types.includes(part[1])) kac_type= true; });\r\n\r\n    if(item.actor.items.find(i=>i.name.includes(`Sniper's Aim`)) && item.data.data.range.value > 5)\r\n    {\r\n      cover = Math.clamped((parseInt(cover)-2), 0, 200);\r\n    }\r\n\r\n    /*\r\n      Check for Save types? --- and what to do with that information?\r\n    */\r\n\r\n    ChatMessage.create({\r\n      content : `To Hit : ${rollData.total} <br> Damage Type : ${damage.parts.map(i=> i[1]).join()} <br> KAC : ${kac.value} <br> EAC : ${eac.value} <br> Cover : ${cover} <br> Concealment : ${concealment} <br> Concealment Roll : ${concealment_roll}`,\r\n      speaker : ChatMessage.getSpeaker(),\r\n      whisper : ChatMessage.getWhisperRecipients(\"GM\"),\r\n      blind : true\r\n    });\r\n\r\n    if(kac_type)\r\n    {\r\n      if(rollData.total >= (kac.value + parseInt(cover)) && ( concealment_roll >= parseInt(concealment)))\r\n      {\r\n        item.rollDamage({event});\r\n      }\r\n    }else{\r\n      if(rollData.total >= (eac.value + parseInt(cover)) && ( concealment_roll >= parseInt(concealment)))\r\n      {\r\n        item.rollDamage({event});\r\n      }\r\n    }\r\n  });\r\n})();\r\n\r\nasync function choose(options = [], prompt = ``)\r\n{\r\n  let value = await new Promise((resolve, reject) => {\r\n\r\n    let dialog_options = (options[0] instanceof Array)\r\n      ? options.map(o => `<option value=\"${o[0]}\">${o[1]}</option>`).join(``)\r\n      : options.map(o => `<option value=\"${o}\">${o}</option>`).join(``);\r\n  \r\n    let content = `${prompt}<br><select id=\"choice\">${dialog_options}</select>`;\r\n  \r\n    new Dialog({\r\n      content, \r\n      buttons : { OK : {label : `OK`, callback : async (html) => { resolve(html.find('#choice').val()); } } }\r\n    }).render(true);\r\n  });\r\n  return value;\r\n}","author":"yWe8HobMv9Lno7mU"},"options":{},"apps":{},"compendium":null}},"core":{"sourceId":"Item.D6z0jkVWroDCjwYY"}},"img":"systems/sfrpg/icons/equipment/weapons/laser-rifle-azimuth.jpg","effects":[]}
